													<!-- Authenticated -->
	
	<bean name="Authenticated1UserAccount" class="acme.framework.entities.UserAccount">
		<property name="username" value="authenticated1" />
		<property name="password" value="authenticated1" />
		<property name="enabled" value="true" />
		<property name="identity">
			<bean class="acme.datatypes.UserIdentity">
				<property name="name" value="Auth" />
				<property name="surname" value="EnticatedOne" />
				<property name="email" value="auth1@corp.com" />
			</bean>
		</property>
		<property name="roles">
			<list>
				<ref bean="Authenticated1" />
			</list>
		</property>
	</bean>
	
	<bean id="Authenticated1" class="acme.framework.entities.Authenticated">
		<property name="userAccount" ref="Authenticated1UserAccount" />
	</bean>
	
	

